<?xml version="1.0" encoding="utf-8"?>
<grammar xml:lang="en-US" version="1.0" root="root"
	xmlns="http://www.w3.org/2001/06/grammar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/06/grammar http://www.iristk.net/xml/srgs.xsd" tag-format="semantics/1.0">

  <rule id="root" scope="public">
		<one-of>
			<item>
				<one-of>
					<item>yes</item>
				</one-of>
				<tag>out.yes=1</tag>
			</item>
			<item>
				<one-of>
					<item>no</item>
				</one-of>
				<tag>out.no=1</tag>
			</item>
			<item>
				<one-of>
					<item>ready</item>
					<item>I'm ready</item>
				</one-of>
				<tag>out.ready=1</tag>
			</item>
			<item>
				<one-of>
					<item>what</item>
				</one-of>
				<tag>out.question=1</tag>
			</item>
		</one-of>
  </rule>
  
</grammar>
